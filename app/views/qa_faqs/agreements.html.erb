<div class="contains">
  <div class="contains-title">
    <h3><%= t('view.qa_faqs/user_agreements.title') %></h3>
  </div>
  <div class="protocol-box">
    <ul>
      <% @qa_faqs.each_with_index do |faq, index| %>
        <li>
          <span><%= format("%02d", show_index(index, @qa_faqs.limit_value)) %></span>
          <%= link_to faq.title, agreement_qa_faq_path(faq) %>
        </li>
      <% end %>
    </ul>
  </div>
</div>