<div class="qa_box">
  <div class="qa_big_cell">
    <span style="padding-left:10px" class="qa_f18">
      <%= link_to LiveStudio::Course.model_name.human, live_studio.teacher_courses_path %> &gt;
      <%= link_to @course.name, live_studio.teacher_course_path(@course) %> &gt;
      <strong><%= LiveStudio::Lesson.model_name.human %></strong>
    </span>
  </div>
</div>

<div class="qa_box">
  <div class="qa_big_cell">
    <%= simple_form_for(@lesson, url: teacher_course_lessons_path(@course)) do |f| %>
      <%= render partial: 'form', locals: {f: f} %>
    <% end %>
  </div>
</div>
