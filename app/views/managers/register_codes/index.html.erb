<div class="qa_box">
  <div class="qa_big_cell">
    <div>
      <h2><%=RegisterCode.model_name.human%>列表
        <%=link_to managers_register_codes_path,:method=> :post do %>
            <button type="button" class="btn btn-xs btn-success"><%="增加#{RegisterCode.model_name.human}" %></button>
        <% end %>
      </h2>

    </div>

  </div>
  <div>
    <%= render partial: "register_code_header" %>
    <%= render partial: "register_code" , collection: @register_codes %>
    <%= will_paginate @register_codes ,renderer: BootstrapPagination::Rails %>

  </div>
</div>