<div class="panel panel-default recordmsg">
  <%= render "student_cash" if @current_resource.student? -%>
  <%= render "teacher_cash" if @current_resource.teacher? -%>
</div>

<%= content_for :javascript do %>
<script type="text/javascript">
$(function() {
  $("#start_date").DatePicker({
    modalMode: false,
    maxDate: moment(),
    endDate: moment()
  });
  $("#end_date").DatePicker({
    modalMode: false,
    maxDate: moment(),
    endDate: moment()
  });
});
</script>
<% end %>
