<div class="qa_box">
  <div class="box_header qa_cell">
    <div class="pull-left">
      <span class="label label-primary">
        <%= I18n.t "view.notification.unread_notification_count", count: @receiver.notifications.unread.count%>
      </span>
    </div>
    <div style="clear:both"></div>
  </div>
  <div class="qa_objects" id="notifications-list">
    <%= render partial: "notification", collection: @notifications %>
  </div>

  <div class="qa_cell">
    <%= will_paginate @notifications %>
  </div>
</div>
