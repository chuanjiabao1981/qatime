<div class="editor-item">
  <h3 class="file-title">设置问题（转述题）</h3>
  <div class="row">
    <div class="col-md-2">
      <label><%= f.object.name %></label>
    </div>
    <div class="col-md-10">
      <%= f.label :title %>
      <%= f.input_field :title %>
      <%= f.error :title %>
      <%= f.label :answer %>
      <%= f.input_field :answer_attach, as: :file %>
      <%= f.error :answer_attach %>
    </div>
  </div>
</div>