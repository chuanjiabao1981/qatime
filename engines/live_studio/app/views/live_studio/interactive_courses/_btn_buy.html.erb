<% if current_user.nil? || current_user.student? %>
  <%= link_to t('activerecord.view.enroll_now'), live_studio.new_interactive_course_order_path(@interactive_course), class: 'btn-addclass action' %>
<% else %>
  <%= link_to t('activerecord.view.enroll_now'), "javascript:alert('#{t('view.live_studio/courses.show.role_notice')}');", class: 'btn-addclass action' %>
<% end %>
