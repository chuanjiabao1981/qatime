<div class="row qa_big_cell">
  <div class="col-md-1">
    <%= waiter_counter + 1 %>
  </div>
  <div class="col-md-2">
    <%= waiter.name%>
  </div>
  <div class="col-md-2">
    <%= waiter.email %>
  </div>
  <div class="col-md-2">
    <%= waiter.mobile %>
  </div>
  <div class="col-md-2">
    <%=link_to '编辑', edit_managers_waiter_path(waiter)%>
  </div>
  <div class="col-md-2">
    <%=link_to '删除', managers_waiter_path(waiter), method: :delete, 'data-confirm' => 'Are you sure?'%>
  </div>
</div>
