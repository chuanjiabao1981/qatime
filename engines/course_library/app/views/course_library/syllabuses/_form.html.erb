<div class="qa_box">
  <div class="qa_big_cell">
    <h2><%= CourseLibrary::Syllabus.model_name.human %></h2>
  </div>
  <div class="qa_big_cell">
    <%=simple_form_for [@teacher,@syllabus] do |f|%>
        <%=f.input :title %>
        <%=f.input :description %>
        <%= f.button :submit, class: "btn btn-default" %>
    <% end %>
  </div>
</div>