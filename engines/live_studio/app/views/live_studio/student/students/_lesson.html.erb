<%= render 'shared/blank', { msg: t('view.tips.week_schedule_none') } if items.blank? %>

<% items.each do |item| %>
  <%= render 'interactive_lesson_item', item: item if item.target.is_a?(LiveStudio::InteractiveLesson) %>
  <%= render 'lesson_item', item: item if item.target.is_a?(LiveStudio::Lesson) %>
<% end unless items.blank? %>
