<div class="qa_box">
  <div class="qa_big_cell">
    <div>
      <h2><%=QaFile.model_name.human%>列表
        <button type="button" class="btn btn-xs btn-success"><%="下载" %></button>
        <button type="button" class="btn btn-xs btn-success"><%="删除" %></button>
      </h2>

    </div>

  </div>
  <div>
    <%=render partial: "qa_file_header" %>
    <%=render partial: "qa_file" , collection: @qa_files %>
    <%= will_paginate @qa_files ,renderer: BootstrapPagination::Rails %>

  </div>
</div>