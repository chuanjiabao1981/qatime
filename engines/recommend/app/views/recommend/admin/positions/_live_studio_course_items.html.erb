        <%= Recommend::LiveStudioCourseItem.model_name.human %>
        <%= link_to :new, new_admin_position_teacher_item_path(@position) %>
      </h2>
    </div>
  </div>
  <div class="row qa_big_cell">
    <div class="col-md-2"><%= Recommend::LiveStudioCourseItem.human_attribute_name(:title) %></div>
    <div class="col-md-2"><%= Recommend::LiveStudioCourseItem.human_attribute_name(:logo) %></div>
    <div class="col-md-2"><%= Recommend::LiveStudioCourseItem.human_attribute_name(:target) %></div>
    <div class="col-md-2"><%= Recommend::LiveStudioCourseItem.human_attribute_name(:owner) %></div>
    <div class="col-md-2"><%= "" %></div>
  </div>
  <%= render @items %>
  <%= will_paginate @items %>
</div>
