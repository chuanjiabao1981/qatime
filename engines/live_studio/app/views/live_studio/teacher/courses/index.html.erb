<%= content_tag(:div, class: 'qa_box panel panel-default panel-support') do %>
  <%= content_tag(:div, class: 'course-header-div') do%>
    <%= content_tag(:div, t("activerecord.view.course"), class: 'title') %>
    <%= content_tag(:div, class: 'search-link') do %>
      <%= link_to helps_courses_path do %>
        <%= t('activerecord.view.helps_course') %>
      <% end %>
    <% end %>
  <% end %>
  <%= content_tag(:div, class: 'course-taxon-list') do %>
    <%= content_tag(:a,content_tag(:div, t("activerecord.view.filter.all"),
                                   class: "course-taxon-div #{@filter_patams.blank? ? 'active' : nil}"), href: teacher_courses_path(@teacher)) %>
    <%= content_tag(:a,content_tag(:div, t("activerecord.view.filter.today"),
                                   class: "course-taxon-div #{@filter_patams[:cate]=='today' ? 'active' : nil}"),href: "?cate=today") %>
    <%= content_tag(:a,content_tag(:div, t("activerecord.view.filter.init"),
                                   class: "course-taxon-div #{@filter_patams[:status]=='init' ? 'active' : nil}"), href: "?status=init") %>
    <%= content_tag(:a,content_tag(:div, t("activerecord.view.filter.preview"),
                                   class: "course-taxon-div #{@filter_patams[:status]=='preview' ? 'active' : nil}"), href: "?status=preview") %>
    <%= content_tag(:a,content_tag(:div, t("activerecord.view.filter.teaching"),
                                   class: "course-taxon-div #{@filter_patams[:status]=='teaching' ? 'active' : nil}"), href: "?status=teaching") %>
    <%= content_tag(:a,content_tag(:div, t("activerecord.view.filter.completed"),
                                   class: "course-taxon-div #{@filter_patams[:status]=='completed' ? 'active' : nil}"), href: "?status=completed") %>
  <% end %>
  <%= content_tag(:div, class: 'course-list') do %>
    <%= render partial: 'course', collection: @courses %>
    <%= will_paginate @courses %>
  <% end %>
  <%= content_tag(:div, class: 'none-list') do %>
    <%= t('view.teacher_course.teacher_course_none_tips') %>
  <% end if @courses.blank? %>
<% end %>

<%= render partial: '/live_studio/courses/publish_tips' %>

