<%= simple_form_for [:manager, @course] do |f|%>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :teacher_id %>
    <%= f.input :description, type: :text %>
    <%= f.input :workstation_id, collection: @workstations %>
    <%= f.input :price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-default" %>
  </div>
<% end %>

