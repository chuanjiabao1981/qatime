
<div class="qa_box">
  <div class="qa_big_cell">
    <h2><%=@software.class.model_name.human%></h2>
  </div>
  <div class="qa_big_cell">
    <%= simple_form_for [:admins, @software] do |f|%>
      <%= f.association :software_category, collection: SoftwareCategory.all.map {|cate| [cate.title, cate.id]} %>
      <%= f.input :logo, as: :file %>
      <%= f.input :version %>
      <%= f.input :description %>
      <%= f.input :cdn_url %>
      <%= f.input :enforce %>
      <%= f.input :position %>
      <%= f.button :submit, class: "btn btn-default" %>
    <% end %>
  </div>
</div>
