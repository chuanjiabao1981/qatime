<div class="contains">
  <div class="contains-title">
    <h3>
      <%= link_to t("view.student_bars.tastes"), live_studio.tastes_student_path(@student) %>
      <span>&gt;</span>
      <span><%= t('view.student.students.tastes.taste_record') %></span>
    </h3>
  </div>

  <div class="contains-discipline">
    <div class="schedule-con audition-con">
      <ul class="calendar-info">
        <%= render partial: "taste_record", collection: @taste_records %>
      </ul>
      <%= render 'shared/blank' if @taste_records.blank? %>
      <center><div class="pagination-fix"><%= will_paginate @taste_records, class: 'paging', previous_label: '←', next_label: '→' %></div></center>
    </div>
  </div>
</div>