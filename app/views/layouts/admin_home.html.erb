<!DOCTYPE html>
<html lang="cn">
<head>
  <meta name="renderer" content="webkit">

  <title>Qatime</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => false %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => false %>
  
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/top_bar' %>

<div class="container">
  <div class='row'>
    <div class='col-md-2'>
      <ul class="menu" style="color: white">


        <li>
          <%= link_to teachers_path do %>
              <span class="glyphicon glyphicon-user"></span> <span class="qa_f18"> 教师</span> Info
          <% end %>
        </li>
        <li>
          <%= link_to admins_cities_path do %>
              <span class="glyphicon glyphicon-map-marker"></span> <span class="qa_f18"> 城市</span> City
          <% end %>
        </li>
        <li>
          <%= link_to schools_path do %>
              <span class="glyphicon glyphicon-home"></span> <span class="qa_f18"> 学校</span> School
          <% end %>
        </li>
        <li>
          <%= link_to questions_path do %>
              <span class="fa fa-university"></span> <span class="qa_f18"><%= VipClass.model_name.human %></span> Vip
          <% end %>
        </li>
        <li>
          <%= link_to students_path do %>
              <span class="glyphicon glyphicon-hdd"></span>
              <span class="qa_f18"> <%= Student.model_name.human %> </span> Student
          <% end %>
        </li>
        <li>
          <%= link_to admins_recharge_codes_path do %>
              <span class="glyphicon glyphicon-credit-card"></span> <span class="qa_f18"> 充值</span> Recharge
          <% end %>
        </li>
        <li>
          <span class="glyphicon glyphicon-info-sign"></span><span class="qa_f18"> 消息</span> Message
        </li>

        <li>
          <%= link_to qa_faqs_path do %>
            <span class="glyphicon glyphicon-question-sign"></span><span class="qa_f18"> <%= QaFaq.model_name.human %></span> Faq
          <% end %>
        </li>



        <li>
          <%= link_to admins_teaching_programs_path do %>
              <span class="glyphicon glyphicon-question-sign"></span>
              <span class="qa_f18">
                   <%=TeachingProgram.model_name.human%>
              </span>
              Program
          <% end %>
        </li>
        <li>
          <%= link_to admins_managers_path do %>
              <span class="glyphicon glyphicon-question-sign"></span>
              <span class="qa_f18">
                人员
              </span>
              Manager
          <% end %>
        </li>

        <li>
          <%= link_to managers_register_codes_path do %>
              <span class="glyphicon glyphicon-question-sign"></span>
              <span class="qa_f18">
                <%= RegisterCode.model_name.human%>
              </span> Code
          <% end %>
        </li>
        <li>
          <%= link_to signout_path, :method=>'delete'  do %>
              <span class="glyphicon glyphicon-log-out"></span> <span class="qa_f18"> 退出</span> Exit
          <% end %>
        </li>
      </ul>
    </div>
    <div class='col-md-10'>
      <%= yield %>
    </div>
  </div>
  <%= debug(params) if Rails.env.development? %>
</div>
<%= render 'layouts/footer' %>

</body>
</html>
