<div class="qa_box">
  <div class="qa_big_cell">
    <h2><%= School.model_name.human %></h2>
  </div>
  <div class="qa_big_cell">
    <%=simple_form_for @school do |f|%>
        <%=f.input :name %>
        <%= f.association :city, :collection => City.all.collect { |c| [c.name,c.id] } ,include_blank: false if current_user.admin?%>
        <%= f.button :submit, class: "btn btn-default" %>
    <% end %>
  </div>
</div>
