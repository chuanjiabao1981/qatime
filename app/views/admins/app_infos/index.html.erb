<div class="qa_box">
  <div class="qa_big_cell">
    <div>
      <h2>
        应用列表
        <%=link_to new_admins_app_info_path do %>
          <button type="button" class="btn btn-xs btn-success">新增应用</button>
        <% end %>
      </h2>
    </div>

  </div>
  <div class="row qa_big_cell">
    <div class="col-md-1">
      序号
    </div>
    <div class="col-md-3">
      应用名称
    </div>
    <div class="col-md-2">
      类型
    </div>
    <div class="col-md-1">
      版本号
    </div>
    <div class="col-md-1">
      状态
    </div>
  </div>
  <div>
    <%= render @app_infos %>
  </div>
</div>