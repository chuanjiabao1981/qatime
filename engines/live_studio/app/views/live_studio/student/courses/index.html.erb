<div class="qa_box">
  <div class="qa_big_cell">
    <div>
      <h2> <%= LiveStudio::Course.model_name.human %>
      </h2>
    </div>
  </div>

  <div>
    <% @courses.each do |course| -%>
    <div class="row qa_big_cell">
      <div class="col-md-1">
        <%= 1 %>
      </div>
      <div class="col-md-2">
        <%= course.name%>
      </div>
      <div class="col-md-2">
        <%= course.teacher_name %>
      </div>
      <div class="col-md-2">
      </div>
      <div class="col-md-4">
        <%= course.short_description %>
      </div>
    </div>
    <% end -%>
  </div>
</div>
