      <span class="label label-primary">
        <%= I18n.t "view.notification.unread_notification_count",count:user.customized_course_action_notifications.unread.count%>
      </span>
    </div>
    <div style="clear:both"></div>
  </div>
  <% if customized_course_action_notifications %>
      <%= render partial: 'notifications/customized_course_action_notification_index_items',
                 locals: {customized_course_action_notifications: customized_course_action_notifications} %>
  <% end %>
</div>