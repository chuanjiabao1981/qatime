<p id="notice"><%= notice %></p>

<h1>Listing Workstation Sellers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workstation_sellers.each do |workstation_seller| %>
      <tr>
        <td><%= link_to 'Show', workstation_seller %></td>
        <td><%= link_to 'Edit', edit_workstation_seller_path(workstation_seller) %></td>
        <td><%= link_to 'Destroy', workstation_seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_workstation_seller_path %>
