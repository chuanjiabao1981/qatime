<div class="qa_box">
  <div class="qa_big_cell">
    <h2>学校</h2>
  </div>
  <div class="qa_big_cell">
      <%=simple_form_for [:admins,@school] do |f|%>
          <%=f.input :name %>
          <%= f.association :city, :collection => City.all.collect { |c| [c.name,c.id] } %>
          <%= f.button :submit, class: "btn btn-default" %>
      <% end %>
  </div>
</div>
