<div class="qa_box">
  <div class="qa_big_cell">
    <h2><%= CourseLibrary::Directory.model_name.human %></h2>
  </div>
  <div>
    <%= render 'nav'%>
  </div>
  <div class="qa_big_cell">
    <%=simple_form_for [@syllabus,@directory] do |f|%>
        <%= f.input :parent_id,as: :hidden%>
        <%= f.input :title %>
        <%= f.button :submit, class: "btn btn-default" %>
    <% end %>
  </div>
</div>