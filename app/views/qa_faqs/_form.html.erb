
<%= content_tag 'div', id:'qa_picture_params',data:{token: @qa_faq.token,picture_type: 'QaFaq'} do %>

<% end %>
<div class="qa_box">
    <div class="qa_big_cell">
      <%= simple_form_for @qa_faq do |f|%>
          <%= f.input :token, as: :hidden %>
          <%= f.input :title %>

        <%= f.input :qa_faq_type,
                    collection:
                    QaFaq.qa_faq_types.inject({}) {
                            |f,(k,v)| f.merge(t("activerecord.attributes.qa_faq.qa_faq_type_enum.#{k}")=>k)
                    },
                    include_blank: false
        %>
        <%= f.input :content ,:input_html => { :data => {:provider => "summernote"} }%>
        <%= f.submit class:"btn btn-default"%>
      <% end %>
    </div>
</div>