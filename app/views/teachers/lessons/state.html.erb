<div class="qa_box">
  <div class="qa_big_cell">
    <ul class="nav nav-tabs">

      <%= render partial: "lesson_state_nav_tab", locals: {state:"editing", teacher_id: params[:teacher_id]} %>

      <%= render partial: "lesson_state_nav_tab", locals: {state:"rejected", teacher_id: params[:teacher_id]} %>

      <%= render partial: "lesson_state_nav_tab", locals: {state:"reviewing", teacher_id: params[:teacher_id]} %>

      <%= render partial: "lesson_state_nav_tab", locals: {state:"published", teacher_id: params[:teacher_id]} %>
    </ul>

    <table class="table table-striped">
      <thead>
        <th width="%5">#</th>
        <th width="60%">
            <%= Lesson.model_name.human %>
        </th>
        <th>
          <%= Course.model_name.human %>
        </th>
      </thead>
      <%= render @lessons %>
    </table>
    <%= will_paginate @lessons %>

  </div>
</div>

