
<% if @change_record.is_a?(Payment::WithdrawRefundRecord) %>
  $('#change_record_tr_<%= @change_record.id %>').html('<%= j render partial: 'shared/workstations/change_records/withdraw_refund' %>');
<% elsif @change_record.target.is_a?(LiveStudio::Lesson) %>
  $('#change_record_tr_<%= @change_record.id %>').html('<%= j render partial: 'shared/workstations/change_records/lesson' %>');
<% elsif @change_record.target.is_a?(LiveStudio::VideoCourse) %>
  $('#change_record_tr_<%= @change_record.id %>').html('<%= j render partial: 'shared/workstations/change_records/video_course' %>');
<% else %>
  $('#change_record_tr_<%= @change_record.id %>').html('<%= j render partial: 'shared/workstations/change_records/customized_course' %>');
<% end %>
