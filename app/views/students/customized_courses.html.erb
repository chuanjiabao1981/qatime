<%= render layout: "shared/customized_courses/index_container",locals:{student: @student} do  %>
    <% if @customized_courses.length > 0 %>
        <%= render partial: 'shared/customized_courses/index', locals:{customized_courses: @customized_courses }%>
    <% else %>
        <%= render 'shared/customized_courses/introduce' %>
    <% end %>
<% end %>
