<div class="qa_box">
  <div class="qa_cell">
    <%= simple_form_for([@group,@course]) do |f|%>
        <%= f.input :name %>
        <%= f.input :price %>
        <%= f.input :desc %>
        <%= f.association :teacher, as: :hidden %>
        <%= f.button :submit, class: "btn btn-default" %>
    <% end %>
  </div>
</div>